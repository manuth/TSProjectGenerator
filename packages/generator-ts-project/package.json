{
  "name": "@manuth/generator-ts-project",
  "version": "1.3.1",
  "description": "A Set of Generators for Creating Projects Written in TypeScript",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yo",
    "yoeoman-generator",
    "Generator",
    "TypeScript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/manuth/TSProjectGenerator/tree/main/packages/generator-ts-project#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/TSProjectGenerator.git",
    "directory": "packages/generator-ts-project"
  },
  "bugs": {
    "url": "https://github.com/manuth/TSProjectGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy-files && npm run compile",
    "copy-files": "cd ../.. && npm run copy-files",
    "compile": "tsc -b tsconfig.build.json",
    "watch-compile": "npm run compile -- --watch",
    "rebuild": "npm run clean && npm run build",
    "watch": "concurrently --raw \"npm run copy-files -- -- --watch | shusher\" \"npm run watch-compile\"",
    "clean": "npm run compile -- --clean && rimraf ./lib",
    "lint-code-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint-code": "npm run lint-code-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-code-ide": "npm run lint-code || exit 0",
    "lint-docs": "markdownlint ./templates/**/*.md",
    "lint": "npm run lint-code && npm run lint-docs",
    "lint-ide": "npm run lint-code-ide && npm run lint-docs || exit 0",
    "test": "mocha",
    "prepare": "npm run rebuild"
  },
  "dependencies": {
    "@manuth/extended-yo-generator": "^8.0.0",
    "@manuth/package-json-editor": "^1.4.6",
    "@manuth/temp-files": "^2.0.4",
    "@types/vscode": "^1.55.0",
    "chalk": "^4.1.1",
    "comment-json": "^4.1.0",
    "dedent": "^0.7.0",
    "detect-newline": "^3.1.0",
    "diff": "^5.0.0",
    "eol": "^0.9.1",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "npm-which": "^3.0.1",
    "parse-pkg-name": "^2.0.0",
    "shusher": "^0.1.1",
    "ts-morph": "^10.0.2",
    "typescript": "^4.2.4",
    "validate-npm-package-name": "^3.0.0",
    "yaml": "^1.10.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@manuth/eslint-plugin-typescript": "^2.3.9",
    "@manuth/extended-yo-generator-test": "^8.0.0",
    "@manuth/tsconfig": "^1.2.9",
    "@manuth/typescript-eslint-plugin": "^1.3.5",
    "@types/dedent": "^0.7.0",
    "@types/diff": "^5.0.0",
    "@types/eslint": "^7.2.10",
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/npm-packlist": "^1.1.1",
    "@types/npm-which": "^3.0.0",
    "@types/validate-npm-package-name": "^3.0.2",
    "@types/yosay": "2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-plugin-deprecation": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.3",
    "markdownlint-cli": "^0.27.1",
    "mocha": "^8.3.2",
    "npm-packlist": "^2.1.5",
    "random-js": "^2.1.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "upath": "^2.0.1",
    "util.chdir": "0.0.43",
    "yo": "^4.0.0"
  }
}
